	"scripts": {
		"prebuild": "rimraf wdist",
		"build": "nest build",
		"start:nodemon:dev": "NODE_ENV=dev && nodemon",
		"build:webpack:dev": "npm run prebuild && webpack --config webpack.dev.config.js --stats-error-details=true",
		"build:webpack:prod": "npm run prebuild && webpack --config webpack.prod.config.js --stats-error-details=true ",
		"build:docker:local": "npm run build:webpack:dev && ./build/build.sh development",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
